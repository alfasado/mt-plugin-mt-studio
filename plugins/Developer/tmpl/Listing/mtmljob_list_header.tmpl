<mt:setvarblock name="system_msg" append="1">
<__trans_section component="Developer">
    <div id="msg-container">
    <mt:if name="request.saved_deleted">
        <mtapp:statusmsg
            id="saved-deleted"
            class="success">
            <__trans phrase="The Custom Handler(s) has been deleted from the database.">
        </mtapp:statusmsg>
    </mt:if>
    <mt:if name="request.not_exported">
        <mtapp:statusmsg
            id="saved"
            class="success">
            <__trans phrase="The Custom Handler(s) has not been exported.">
        </mtapp:statusmsg>
    </mt:if>
    </div>
</__trans_section>
</mt:setvarblock>